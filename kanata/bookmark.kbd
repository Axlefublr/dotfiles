(deflayermap (bookmark)
	a (cmd $fsh "bookmark_open a")
	b (cmd $fsh "bookmark_open b")
	c (cmd $fsh "bookmark_open c")
	s (tap-hold-release $tt $ht (cmd $fsh "bookmark_open s") (layer-while-held bookmark-shifted))
	e (cmd $fsh "bookmark_open e")
	f (cmd $fsh "bookmark_open f")
	g (cmd $fsh "bookmark_open g")
	h (cmd $fsh "bookmark_open h")
	i (cmd $fsh "bookmark_open i")
	j (cmd $fsh "bookmark_open j")
	l (tap-hold-release $tt $ht (cmd $fsh "bookmark_open l") (layer-while-held bookmark-shifted))
	k (cmd $fsh "bookmark_open k")
	m (cmd $fsh "bookmark_open m")
	n (cmd $fsh "bookmark_open n")
	o (cmd $fsh "bookmark_open o")
	p (cmd $fsh "bookmark_open p")
	q (cmd $fsh "bookmark_open q")
	r (cmd $fsh "bookmark_open r")
	d (cmd $fsh "bookmark_open d")
	t (cmd $fsh "bookmark_open t")
	u (cmd $fsh "bookmark_open u")
	v (cmd $fsh "bookmark_open v")
	w (cmd $fsh "bookmark_open w")
	x (cmd $fsh "bookmark_open x")
	y (cmd $fsh "bookmark_open y")
	z (cmd $fsh "bookmark_open z")
	[ (cmd $fsh "bookmark_open leftbrace")
	] (cmd $fsh "bookmark_open rightbrace")
	; (cmd $fsh "bookmark_open semicolon")
	' (cmd $fsh "bookmark_open apostrophe")
	, (cmd $fsh "bookmark_open comma")
	. (cmd $fsh "bookmark_open period")
	/ (cmd $fsh "bookmark_open slash")
)

