(deflayermap (nuclear-throne)
	caps (multi (layer-switch default) (cmd notify-send default))
	- (multi (layer-switch pvz) (cmd notify-send "plants vs zombies"))
	= (multi (layer-switch slay-the-spire) (cmd notify-send "slay the spire"))
	9 (cmd fish -c screenshot_window)
	7 (tap-hold 0 100 XX (macro / 20 / u n l o a d m o d spc g r i l l s k i l l s ret))
	8 (tap-hold 0 100 XX (macro / 20 / l o a d m o d spc g r i l l s k i l l s ret))
)

(deflayermap (slay-the-spire)
	caps (multi (layer-switch default) (cmd notify-send default))
	- (multi (layer-switch nuclear-throne) (cmd notify-send "nuclear throne"))
	= (multi (layer-switch enter-the-gungeon) (cmd notify-send "enter the gungeon"))
	9 (cmd fish -c screenshot_window)
	j 1
	k 2
	l 3
	u 4
	i 5
	o 6
	m 7
	, 8
	. 9
	; 0
	' e
)

(deflayermap (enter-the-gungeon)
	caps (multi (layer-switch default) (cmd notify-send default))
	- (multi (layer-switch slay-the-spire) (cmd notify-send "slay the spire"))
	= (multi (layer-switch pvz) (cmd notify-send "plants vs zombies"))
	9 (cmd fish -c screenshot_window)
	1 (tap-hold 0 100 XX (cmd-output-keys etg.nu "stat add curse 5" "stat add coolness 5" "give brick_of_cash" "add_teleporters"))
	2 (tap-hold 0 100 XX (cmd-output-keys etg.nu --active))
	3 (tap-hold 0 100 XX (cmd-output-keys etg.nu "add_teleporters"))
	4 (tap-hold 0 100 XX (cmd-output-keys etg.nu "give old_crest"))
	5 (tap-hold 0 100 XX (cmd-output-keys etg.nu "give gnawed_key"))
	6 (tap-hold 0 100 XX (cmd-output-keys etg.nu "give coolant_leak"))
	7 (tap-hold 0 100 XX (cmd-output-keys etg.nu "give gunknight_helmet" "give gunknight_armor"))
	8 (tap-hold 0 100 XX (cmd-output-keys etg.nu "give gungeon_blueprint"))
)

(deflayermap (pvz)
	caps (multi (layer-switch default) (cmd notify-send default))
	- (multi (layer-switch enter-the-gungeon) (cmd notify-send "enter the gungeon"))
	= (multi (layer-switch nuclear-throne) (cmd notify-send "nuclear throne"))
	9 (cmd fish -c screenshot_window)
	j (cmd fish -c "pvz_plant 252")
	k (cmd fish -c "pvz_plant 313")
	l (cmd fish -c "pvz_plant 368")
	u (cmd fish -c "pvz_plant 433")
	i (cmd fish -c "pvz_plant 491")
	o (cmd fish -c "pvz_plant 550")
	m (cmd fish -c "pvz_plant 612")
	h (cmd fish -c "pvz_plant 684")
)
