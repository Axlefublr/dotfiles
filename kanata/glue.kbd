(deflayermap (#←)
	f (multi (layer-while-held arrow) (layer-while-held #arrow) (on-release tap-vkey any-arrow))
	e (multi (layer-while-held hack) (layer-while-held #hack) (on-release tap-vkey any-hack))
)

(deflayermap (!←)
	g (layer-while-held !apps←)
)

(deflayermap (!→)
	h (layer-while-held !apps→)
)

(deflayermap (#arrow)
	e (multi (layer-while-held hack) (layer-while-held #hack) (layer-while-held #hack-arrow) (on-release tap-vkey any-hack))
)
