(deflayermap (symbol-primary)
	;; [[sort on]]
	, S-1
	. (cmd wtype …)
	/ bksl
	[ S-9
	] S-0
	h S-4
	i S-8
	j S-eql
	k min
	l eql
	m S-5
	o S-3
	u grv
	;; [[sort off]]
)

(deflayermap (symbol-secondary)
	;; [[sort on]]
	, (cmd wtype “)
	. (cmd wtype ”)
	/ S-bksl
	i S-6
	k S-min
	l (cmd wtype —)
	m S-grv
	o S-2
	p (cmd wtype ™)
	u S-7
	;; [[sort off]]
)

(deflayermap (symbol-tertiary)
	;; [[sort on]]
	, (cmd wtype ‼️)
	/ (cmd wtype ❓)
	h (cmd wtype ←)
	j (cmd wtype ↓)
	k (cmd wtype ↑)
	l (cmd wtype →)
	n (cmd wtype ❌)
	o (cmd wtype ✅)
	;; [[sort off]]
)

(deflayermap (symbol-left)
	;; [[sort on]]
	a (cmd wtype 👏)
	c (cmd wtype 😭)
	e (cmd wtype 🗣️)
	f (cmd wtype 🔥)
	g (cmd wtype 🐐)
	r (cmd wtype 🚀)
	s (cmd wtype 💀)
	t (cmd wtype 🤔)
	w (cmd wtype ✍️)
	x (cmd wtype 🤯)
	;; [[sort off]]
)

(deflayermap (symbol-right)
	;; [[sort on]]
	n (cmd wtype 🥺)
	u (cmd wtype 😋)
	y (cmd wtype 🥳)
	;; [[sort off]]
)
