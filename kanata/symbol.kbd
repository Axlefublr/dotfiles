(deflayermap (symbol-primary)
	;; [[sort on]]
	' S-0
	, S-1
	. (cmd wtype …)
	/ bksl
	; S-9
	c (layer-while-held symbol-quadruple)
	h S-4
	i S-8
	j S-eql
	k min
	l eql
	m S-5
	o S-3
	p (cmd wtype °)
	;; [[sort off]]
)

(deflayermap (symbol-secondary)
	x (layer-while-held symbol-quadruple)
	;; [[sort on]]
	' S-]
	, (cmd wtype “)
	. (cmd wtype ”)
	/ S-bksl
	; S-[
	h (cmd wtype €)
	i S-6
	j grv
	k S-min
	l (cmd wtype —)
	m S-grv
	o S-2
	p (cmd wtype ™)
	u S-7
	;; [[sort off]]
)

(deflayermap (symbol-tertiary)
	;; [[sort on]]
	' ]
	, (cmd wtype ❗)
	/ (cmd wtype ❓)
	; [
	b (cmd wtype 💡)
	h (cmd wtype ←)
	j (cmd wtype ↓)
	k (cmd wtype ↑)
	l (cmd wtype →)
	n (cmd wtype ❌)
	o (cmd wtype ✅)
	p (cmd wtype √)
	u (cmd wtype ₽)
	;; [[sort off]]
)

(deflayermap (symbol-quadruple)
	, (cmd wtype «)
	. (cmd wtype »)
	i (cmd wtype ‘)
	o (cmd wtype ’)
)

(deflayermap (symbol-left)
	;; [[sort on]]
	a (cmd wtype 🙏)
	c (cmd wtype 😭)
	d (cmd wtype 💃)
	e (cmd wtype 🗣️)
	f (cmd wtype 🔥)
	g (cmd wtype 🐐)
	r (cmd wtype 🚀)
	s (cmd wtype 💀)
	t (cmd wtype 🤔)
	w (cmd wtype ✍️)
	x (cmd wtype 🤯)
	;; [[sort off]]
)

(deflayermap (symbol-right)
	;; [[sort on]]
	k (cmd wtype ✨)
	l (cmd wtype 😌)
	m (cmd wtype 😩)
	n (cmd wtype 🥺)
	p (cmd wtype 👏)
	u (cmd wtype 😋)
	y (cmd wtype 🥳)
	;; [[sort off]]
)
